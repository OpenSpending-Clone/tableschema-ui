{
  "name": "tableschema-ui",
  "version": "0.1.0",
  "license": "MIT",
  "author": "OKI",
  "description": "UI components for tableschema",
  "homepage": "https://github.com/frictionlessdata/tableschema-ui",
  "bugs": "https://github.com/frictionlessdata/tableschema-ui/issues",
  "repository": "github:frictionlessdata/tableschema-ui",
  "main": "lib/index.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "scripts": {
    "build": "webpack",
    "build:prod": "NODE_ENV=production npm run build",
    "compile": "babel src --out-dir lib --copy-files",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "webpack-dev-server --inline --hot",
    "e2e": "NODE_ENV=testing node e2e/runner.js",
    "lint": "eslint src test",
    "posttest": "npm run e2e",
    "prepush": "npm run test",
    "pretest": "npm run lint",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "NODE_ENV=testing nyc mocha",
    "test:browser": "NODE_ENV=testing karma start"
  },
  "dependencies": {
  },
  "devDependencies": {
  }
}
