{
  "name": "tableschema-ui",
  "version": "0.1.1",
  "license": "MIT",
  "author": "OKI",
  "description": "UI components for tableschema",
  "homepage": "https://github.com/frictionlessdata/tableschema-ui",
  "bugs": "https://github.com/frictionlessdata/tableschema-ui/issues",
  "repository": "github:frictionlessdata/tableschema-ui",
  "main": "lib/index.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "test"
  ],
  "scripts": {
    "build": "webpack",
    "build:prod": "NODE_ENV=production npm run build",
    "compile": "babel src --out-dir lib --copy-files",
    "compile:prod": "NODE_ENV=production npm run compile",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dev": "webpack-dev-server",
    "lint": "eslint src test",
    "prepush": "npm run test",
    "pretest": "npm run lint",
    "profile": "webpack --profile --json | webpack-bundle-size-analyzer",
    "profile:prod": "NODE_ENV=production npm run profile",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "NODE_ENV=testing nyc mocha",
    "test:browser": "NODE_ENV=testing karma start"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "@babel/register": "^7.0.0-beta.40",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-istanbul": "^4.1.5",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "jsdom": "^11.6.2",
    "karma": "^2.0.0",
    "karma-jsdom-launcher": "^6.1.3",
    "karma-mocha": "^1.3.0",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^2.0.13",
    "mocha": "^5.0.4",
    "nyc": "^11.4.1",
    "react-hot-loader": "^4.0.0",
    "sinon": "^4.4.3",
    "style-loader": "^0.20.3",
    "webpack": "^4.1.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1"
  }
}
